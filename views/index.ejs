<html>
  <head>
    <meta charset="utf-8" />
    <title>PokeDex</title>
  </head>
  <body>
    <h1>PokeDex</h1>
    <ul>
      <% for (let i = 0; i < allPokemon.length; i++ ) { %>
        <li>      
          <a href="/pokemon/<%= i %>"><%=allPokemon[i].name %></a>
          <img src="<%= allPokemon[i].img %>" alt="<%= allPokemon[i].name" %>">
          </li>

    <% } %>
        </ul>
    </ul>
  </body>
</html>